{
  "name": "dict2json",
  "icon": "images/icon.png",
  "displayName": "dict2json",
  "description": "Convert Python Dict to Json-formatted Dict",
  "publisher": "noamlstuff",
  "version": "0.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoamLoewenstern/dict2json"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dict2json.convertPythonDictToJson",
        "title": "dict2json: Convert Python-Dict To Json"
      }
    ],
    "configuration": {
      "title": "Dict2Json",
      "properties": {
        "dict2json.apiUrl": {
          "type": "string",
          "default": "https://pythondict2json-kwwxoeu7ra-uc.a.run.app",
          "description": "The URL of the dict2json api-service"
        },
        "dict2json.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "The Python path to use for running the dict2json script"
        }
      }
    }
  },
  "files": [
    "out",
    "pythonScripts"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.60.0",
    "@types/which": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "which": "^3.0.0"
  }
}